    "scripts": {
        "contracts:migrate": "bash contract-scripts/migrate_dharma_contracts.sh",
        "chain": "ganache-cli -m 'pink proof foil skull tide narrow audit card rotate forget liar favorite' -i 1457",
        "deploy": "aws s3 sync build s3://plex.dharma.io --delete --region us-west-2 --cache-control 'no-cache, no-store' --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
        "deploy:staging": "aws s3 sync build s3://staging-plex.dharma.io --delete --region us-west-2 --cache-control 'no-cache, no-store' --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
        "start": "node scripts/start.js --history-api-fallback",
        "build": "node scripts/build.js",
        "test:watch": "jest --watch --runInBand",
        "ngrok:start": "ngrok http 3000",
        "migrations:loans": "tsc; node build/dist/migrations/loans.js",
        "prettify": "prettier --write 'src/**/*.ts{,x}'",
        "precommit": "yarn prettify"
    },
    "devDependencies": {
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "ganache-cli": "^6.1.0",
        "husky": "^0.14.3",
        "jest-enzyme": "^4.2.0",
        "prettier": "1.11.1",
        "redux-mock-store": "1.4.0",
        "ts-jest": "^22.0.4",
        "typescript": "^2.7.1",
        "web3-typescript-typings": "^0.9.11"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__test__/.*|\\.(test|spec))\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "modulePaths": [
            "<rootDir>/"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/test/"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/__test__/setup.js",
            "/build/dist/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 30,
                "functions": 40,
                "lines": 40,
                "statements": 40
            }
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{ts,tsx}",
            "!**/styledComponents.tsx",
            "!**/*Container.{ts,tsx}",
            "!**/models/index.ts",
            "!**/modules/index.ts",
            "!**/modules/Welcome/index.ts",
            "!**/modules/Dashboard/index.ts",
            "!**/modules/FillLoan/index.ts",
            "!**/modules/FillLoan/FillLoanEntered/index.ts",
            "!**/modules/RequestLoan/index.ts",
            "!**/modules/RequestLoan/RequestLoanForm/index.ts",
            "!**/components/Form/JSONSchemaForm/typeDef.ts"
        ],
        "mapCoverage": true,
        "setupTestFrameworkScriptFile": "./__test__/setup.js",
        "unmockedModulePathPatterns": [
            "react",
            "enzyme",
            "jest-enzyme"
        ],
        "testURL": "http://localhost"
    }
}
